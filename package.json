{
  "name": "@eq8/mvp",
  "version": "0.0.0",
  "description": "TBD",
  "main": "index.js",
  "bin": {
    "mvp": "./bin/mvp"
  },
  "scripts": {
    "install": "npm-license-crawler > ./NOTICE.txt",
    "precommit": "npm run test:lint",
    "prepush": "npm run test:lint && npm run test:unit",
    "start": "nodemon",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint --cache --ext .js index.js bin/* plugins",
    "test:unit": "tape test/test.unit.js | faucet",
    "test:integration": "tape test/test.integration.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eq8/mvp.git"
  },
  "keywords": [
    "eq8",
    "mvp"
  ],
  "author": "bbartolome",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eq8/mvp/issues"
  },
  "homepage": "https://github.com/eq8/mvp#readme",
  "dependencies": {
    "commander": "2.15.1",
    "express": "4.15.3",
    "express-graphql": "0.6.12",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.2",
    "immutable": "3.8.1",
    "lodash": "4.17.9",
    "lru-cache": "4.1.2",
    "passport": "0.4.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "requirejs": "2.3.5",
    "rethinkdb": "2.3.3",
    "semver": "5.5.0",
    "url-parse": "1.4.1",
    "uuid": "3.2.1",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "eslint": "5.0.1",
    "eslint-config-eslint": "4.0.0",
    "faucet": "0.0.1",
    "husky": "0.14.3",
    "nodemon": "1.17.5",
    "npm-license-crawler": "0.1.9",
    "tape": "4.9.1"
  },
  "nodemonConfig": {
    "exec": "docker-compose up --build",
    "ignore": [
      "test/*",
      "docs/*"
    ]
  }
}
