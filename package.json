{
  "name": "@eq8/mvp",
  "version": "0.0.0",
  "description": "TBD",
  "main": "index.js",
  "bin": {
    "mvp": "./bin/mvp"
  },
  "scripts": {
    "precommit": "npm run test:lint",
    "prepush": "npm run test:lint && npm run test:unit",
    "start": "nodemon",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint --cache --ext .js index.js bin/* lib",
    "test:unit": "tape test/test.unit.js | faucet",
    "test:integration": "tape test/test.integration.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eq8/mvp.git"
  },
  "keywords": [
    "eq8",
    "mvp"
  ],
  "author": "bbartolome",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eq8/mvp/issues"
  },
  "homepage": "https://github.com/eq8/mvp#readme",
  "dependencies": {
    "@eq8/mvp-boot": "git+https://git@github.com/eq8/mvp-boot.git#master",
    "lodash": "4.17.9",
    "request": "2.87.0",
    "url-parse": "1.4.1",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "eslint": "5.0.1",
    "eslint-config-eslint": "4.0.0",
    "faucet": "0.0.1",
    "husky": "0.14.3",
    "nodemon": "1.18.3",
    "tape": "4.9.1"
  },
  "nodemonConfig": {
    "exec": "docker-compose up --build --remove-orphans",
    "ignore": [
      "test/*",
      "docs/*"
    ]
  }
}
